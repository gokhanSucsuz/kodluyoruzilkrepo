import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        byte sicaklik;
        System.out.println("Lütfen sıcaklık derecesini giriniz...");
        Scanner scan = new Scanner(System.in);
        sicaklik = scan.nextByte();
            if (sicaklik < 5) System.out.println("Kayak yapabilirsiniz...");
                else if (sicaklik > 5 && sicaklik <= 15) System.out.println("Sinemaya gidebilirsiniz...");
                else if (sicaklik <= 25) System.out.println("Piknik yapabilirsiniz...");
                else System.out.println("Yüzmeye gidebilirsiniz...");
            }

        }
